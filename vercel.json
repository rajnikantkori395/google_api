{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/sheets/:spreadsheetId/:range?",
      "dest": "./controllers/sheetsController.js"
    },
    {
      "src": "/sheets/([^/]+)/([^/]+)?",
      "dest": "./index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
